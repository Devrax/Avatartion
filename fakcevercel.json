{
  "public": true,
  "outputDirectory": "server/static",
  "installCommand": "yarn",
  "buildCommand": "yarn build",
  "builds": [
    {
      "src": "server/main.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/main.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "server/static/index.html"
    }
  ]
}